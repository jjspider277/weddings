                        <div id="change_user_pass_form_container" >
                            {{ render(path('security_crud_user_change_pass'))}}
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn default" data-dismiss="modal">Cerrar</button>
                        <button class="btn green " type="submit"  value="Aceptar">Aceptar</button>
                    </div>
                </form>
            </div>
            <!-- /.modal-content -->
        </div>
    </div>
{% block pagecontent %}

    <div class="page-content" style="margin-left: 0px;box-shadow: 4px 4px 10px #999;">
        <div class="page-content-body">
            {% block breadcrumbs %}
            {% endblock %}

